{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "space-before-function-paren": "off",
    "comma-dangle": "warn",
    "no-param-reassign": ["warn"],
    "global-require": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-console": "off"
  }
}
